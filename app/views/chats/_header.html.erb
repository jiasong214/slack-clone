<div class="header">
  <h2 class="header__title">
    <div class="channel__infoBtn"><%= @channel.name %></div>
  </h2>
  <div class="header__member">
    <% 3.times do |i| %>
      <% break unless @channel.users[i].present?%>
      <div class="img-container">
        <%= cl_image_tag @channel.users[i].image, alt: "user image" %>
      </div>
    <% end %>
    <span class="member__count"><%= @channel.users.count %></span>
  </div>
</div>